/* pages/buySuccess/buySuccess.wxss */
.container{width:750rpx;padding:30rpx;box-sizing:border-box;}
.header{padding-bottom:30rpx;border-bottom:solid 1rpx lightgray;}
.goods_img{width:154rpx;height:118rpx;margin-right:34rpx;background:greenyellow;border-radius:10rpx;}
.goods_title{font-size:32rpx;}
.picbox{margin-top:24rpx;align-items:baseline;}
.newpic{margin-right:30rpx;color:#747474;}
.red{color:#F24747;font-size:28rpx;}
.delete{text-decoration:line-through;color:#BCBCBC;font-size:22rpx;}

.goods_img2{width:100%;height:320rpx;margin-top:30rpx;border-radius:10rpx;}

.btn{width:240rpx;height:84rpx;border-radius:42rpx;color:#ffffff;font-size:26rpx;text-align:center;line-height:84rpx;}
.ok{background:linear-gradient(180deg,#59D1F1,#178ED2);}
.clear{background:linear-gradient(180deg,#A8C6CF,#517E90);margin-left:100rpx;}
.m_t30{margin-top:30rpx;}
.m_t10{margin-top:10rpx;}